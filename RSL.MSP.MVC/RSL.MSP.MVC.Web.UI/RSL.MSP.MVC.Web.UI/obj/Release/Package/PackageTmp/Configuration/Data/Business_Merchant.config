  <dataCommand name="getMerchantSeq" database="MSP_SYS" commandType="Text">
    <commandText>
      <![CDATA[
       SELECT SEQ_MERCHANT.NEXTVAL FROM DUAL
				  ]]>
    </commandText>
  </dataCommand>

  <dataCommand name="insertMerchantTemp" database="MSP_SYS" commandType="Text">
